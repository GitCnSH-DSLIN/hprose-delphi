{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Hprose for Delphi XE8+'}
{$IMPLICITBUILD OFF}

requires
  rtl,
  IndyCore,
  IndyProtocols,
  IndySystem;

contains
  ObjAutoX in '..\Source\ObjAutoX.pas',
  HproseCommon in '..\Source\HproseCommon.pas',
  HproseIO in '..\Source\HproseIO.pas',
  HproseClient in '..\Source\HproseClient.pas',
  HproseHttpClient in '..\Source\XE8\HproseHttpClient.pas';

end.
